<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cn.tedu.csmall.server.mapper.AttributeTemplateMapper">

    <!--int deleteAttributeTemplateById(AttributeTemplate attributeTemplate);-->
    <delete id="deleteAttributeTemplateById">
        delete from pms_attribute_template where id=#{id}
    </delete>

    <!--int insertAttributeTemplate(AttributeTemplate attributeTemplate);-->
    <insert id="insertAttributeTemplate" useGeneratedKeys="true" keyProperty="id">
        insert into pms_attribute_template (
            name,pinyin,keywords,sort,gmt_create,gmt_modified
        ) values (
            #{name},#{pinyin},#{keywords},#{sort},#{gmtCreate},#{gmtModified}
                 )
    </insert>
</mapper>
